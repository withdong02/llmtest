<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.llmtest.mapper.TestQuestionsMapper">

    <!-- 批量插入测试题目关联关系 -->
    <insert id="insertBatch" parameterType="java.util.List">
        INSERT INTO test_questions (test_id, data_id, model_output)
        VALUES
        <foreach collection="list" item="item" separator=",">
            (#{item.testId}, #{item.dataId}, #{item.modelOutput})
        </foreach>
    </insert>

    <insert id="insertBatchWithoutScore" parameterType="java.util.List">
        INSERT INTO test_questions (test_id, data_id)
        VALUES
        <foreach collection="list" item="item" separator=",">
            (#{item.testId}, #{item.dataId})
        </foreach>
    </insert>
</mapper>
